<launch>

    <!-- Bounds Checker parameters -->
    <param name="dist_lower_bound" type="double" value="0.8" />
    <param name="dist_upper_bound" type="double" value="5.41"/>
    <param name="dist_lower_bound_min" type="double" value="0.1" />
    <param name="dist_upper_bound_max" type="double" value="35.0" />

    <!-- Watchdog Pulse parameters -->
    <param name="watchdog_update_rate" type="double" value="10"/>

    <!-- Motor command values -->
    <param name="motor_cmd_max_value" type="int" value="4095"/>

    <!-- Joystick control parameters -->
    <param name="joystick_max_velo_default" type="double" value="30.0"/>

    <!-- System update rate -->
    <param name="update_rate" type="double" value="50.0"/>

</launch>
